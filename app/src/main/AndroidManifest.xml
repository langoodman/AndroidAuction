<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.ctguwh.auction.client">
	<!-- 授权访问网络 -->
	<uses-permission android:name="android.permission.INTERNET"/>
	<application
		android:allowBackup="true"
		android:usesCleartextTraffic="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity
			android:name="org.ctguwh.auction.client.LoginActivity"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.AuctionClientActivity"
			android:label="@string/app_name">
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.ViewItemActivity"
			android:label="@string/app_name">
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.ManageKindActivity"
			android:label="@string/manage_kind">
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.AddKindActivity"
			android:label="@string/add_kind_title">
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.ManageItemActivity"
			android:label="@string/manage_item">
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.AddItemActivity"
			android:label="@string/add_item_title">
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.ViewBidActivity"
			android:label="@string/view_bid">
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.ChooseKindActivity"
			android:label="@string/choose_kind">
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.ChooseItemActivity"
			android:label="@string/app_name">
		</activity>
		<activity
			android:name="org.ctguwh.auction.client.AddBidActivity"
			android:label="@string/item_detail_title">
		</activity>
	</application>

</manifest>